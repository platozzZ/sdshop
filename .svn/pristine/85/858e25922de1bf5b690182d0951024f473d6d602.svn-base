.head {
	height: 88upx;
	padding: 0 30upx;
	border-bottom: 1px solid #eee;
	font-size: 40upx;
	color: #242424;
	display: flex;
	justify-content: space-between;
	align-items: center;
}

.head_img {
	width: 24upx;
	height: 36upx;
}


.getGray {
	border: 1px solid #999999 !important;
	color: #999999 !important;
}

.dayNum {
	display: inline-block;
	text-align: center;
	border-radius: 5upx;
}

.group_num {
	font-size: 22upx;
	color: #999999;
	position: absolute;
	bottom: 30upx;
}

.g_n_num {
	background: rgba(2, 2, 2, 1);
	border-radius: 6upx;
	width: 38upx;
	display: inline-block;
	text-align: center;
	color: #fff;
	height: 34upx;
	line-height: 34upx;
}

.g_n_nums {
	background:rgba(153,153,153,1) !important;
}

.g_n_dian {
	margin: 0upx 4upx;
	color: #020202;
}

.brBtn {
	font-size: 30upx;
	height: 56upx;
	width: 140upx;
	display: flex;
	align-items: center;
	justify-content: center;
	border: 1px solid #242424;
	color: #242424;
	border-radius: 5upx;
	position: absolute;
	right: 0;
	bottom: 30upx;
	font-weight: bold;
}

.brNum {
	color: #242424;
	font-size: 24upx;
	position: absolute;
	bottom: 0;
}

.oldPrice {
	font-size: 24upx !important;
	text-decoration: line-through;
}

.newPrice {
	color: #FF1818;
	font-size: 35upx;
	margin: 0 10upx;
	font-weight: bold;
}

.proPrice {
	color: #999999;
	font-size: 26upx;
	position: absolute;
	top: 0;
	bottom: 0;
	display: flex;
	align-items: center;
	flex-wrap: wrap;
	align-content: center
}

.proPrice1 {
	color: #ff1818;
	font-size: 26upx;
	margin-top: 5upx;
}

.proTitle {
	font-size: 28upx;
	color: #000000;
	font-weight: bold;
}

.rightBrDiv {
	width: 490upx;
	height: 230upx;
	text-align: left;
	position: relative;
}

.leftBrDiv {
	width: 150upx;
	height: 200upx;
	margin-right: 30upx;
}

.leftBrDiv img {
	width: 150upx;
	height: 150upx;
}

.brDiv {
	background-color: #fff;
	padding: 20upx 40upx;
	height: 270upx;
	width: 100%;
	display: flex;
	align-items: center;
	padding-top: 40upx;
	position: relative;
}

.topTabBar {
	height: 90upx;
	display: flex;
	align-items: center;
	justify-content: space-around;
	border-bottom: 1px solid #eee;
	font-size: 30upx;
	background-color: #fff;
	color: #999999;
}

.topTabBar div {
	height: 100%;
	display: flex;
	align-items: center;
	padding: 0 10upx;
	justify-content: center;
}

.active {
	border-bottom: 2px solid #000000;
	color: #242424 !important;
	font-weight: 800;
}

.no-bargain {
	pointer-events: none;
	width: 100%;
	text-align: center;
	padding-top: 199upx;
	background-color: rgb(246, 246, 246);
}

.no-bargain p {
	height: 29upx;
	line-height: 36upx;
	color: #666666;
	font-size: 30upx;
	font-weight: 500;
}

.no-bargain img {
	display: block;
	width: 240upx;
	height: 238upx;
	margin: auto;
	margin-bottom: 45upx;
}

.spacer {
	height: 10upx;
	background-color: #f6f6f6;
	width: 100%;
	position: absolute;
	bottom: 0;
	left: 0;
}

.btn-fixed {
	display: flex;
	position: fixed;
	right: 30upx;
	bottom: 120upx;
	width: 80upx;
	height: 80upx;
	border-radius: 50%;
	background-color: rgba(36, 36, 36, .8);
	color: #fff;
	font-size: 25upx;
	line-height: 30upx;
	padding: 10upx;
	justify-content: center;
	align-items: center;
}

.btn-fixed::after {
	border: 0;
}

.mt-2 {
	margin-top: 20upx;
}

.uni-modal {
	border-radius: 23upx;
}

.uni-modal__hd {
	border-bottom: 1px #eee solid;
}

.dialog {
	position: fixed;
	top: 0;
	right: 0;
	bottom: 0;
	left: 0;
	background-color: rgba(0, 0, 0, .4);
	z-index: 999;
}

.dialog .dialog-box {
	width: 520upx;
	min-height: 288upx;
	background-color: #fff;
	border-radius: 23upx;
	margin: 468upx auto auto;
}

.dialog .dialog-header {
	height: 78upx;
	line-height: 78upx;
	text-align: center;
	font-size: 32upx;
	font-weight: bold;
	color: rgba(36, 36, 36, 1);
	border-bottom: 4upx rgb(238, 238, 238) solid;
}

.dialog .dialog-content {
	line-height: 44upx;
	min-height: 126upx;
	padding: 22upx 100upx;
	font-size: 26upx;
	font-weight: 500;
	color: rgb(68, 68, 68);
	text-align: center;
	box-sizing: border-box;
}

.dialog .dialog-footer {
	height: 80upx;
	border-top: 1upx rgb(238, 238, 238) solid;
	box-sizing: border-box;
	display: flex;
	justify-content: center;
}

.dialog .dialog-footer view {
	width: 50%;
	height: 78upx;
	line-height: 78upx;
	color: #888888;
	font-size: 30upx;
	text-align: center;
}

.dialog .dialog-footer .dialog-confirm {
	border-left: 1upx rgb(238, 238, 238) solid;
	color: rgba(0, 0, 0, 1);
}

.fade-enter-active,
.fade-leave-active {
	transition: all .3s;
}

.fade-enter,
.fade-leave-to {
	transform: scale(1.2, 1.2);
	opacity: 0;
}

.heads {
	display: flex;
	width: 100%;
	height: 88upx;
	line-height: 88upx;
	align-items: flex-start;
	justify-content: flex-start;
	align-items: baseline;
	background-color: #fff;
	border-bottom: 1px solid #eee;
	z-index: 9999;
	box-sizing: initial;
}

.heads .heads-left,
.heads .head-right {
	width: 60upx;
	min-width: 60upx;
	height: 88upx;
	text-align: center;
}

.heads-centent {
	width: 630upx;
	text-align: center;
	font-size: 32upx;
	color: #020202;
}

.heads-img {
	width: 24upx;
	height: 36upx;
}

.data_h_h {
	position: fixed;
	top: 0;
	left: 0;
	right: 0;
	background: #FFFFFF;
	z-index: 9999;
}

.yh-length {
	width: 320upx;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}

/* 优化 */
.yh-page {
	min-height: 100vh;
	background-color: #f6f6f6;
}

.yh-brHeadImg {
	height: 320upx;
	width: 100%;
}

.yh-day {
	margin: 0 5upx;
	color: #020202;
}

.yh-g-n-num {
	margin-right: 10upx;
}