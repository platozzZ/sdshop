	.order_two{justify-content: space-between;text-align: center; color: #666666;}
	.order_b{width:32upx;height: 32upx;border-radius: 50%;position: absolute;font-size: 12px;top:-25upx;right: 1upx;background-color: #FF0000;color: #FFFFFF;
				display: flex;justify-content: center;align-items: center;}
	.order_plus{width:32upx;height: 32upx;border-radius: 50%;position: absolute;font-size: 12px;top:-10upx;right: -13upx;background-color: #FF0000;color: #FFFFFF;
				display: flex;justify-content: center;align-items: center;}
	.order_two img{width: 44upx;height: 40upx;margin-bottom: 15upx;margin-right: 0;}
	.list_ul{display: flex; align-items: center;justify-content: flex-start;flex-wrap:wrap;background-color: #fff;}
	.list_ul li{font-size: 28upx;color: #020202;border-bottom: 1px solid #EEEEEE;width: 33%;height: 200upx;
	border-right: 1px solid #eee;display: flex;align-items: center;justify-content: center;text-align: center;}
	.list_ul li>div img{width: 48upx;height: 48upx;margin-bottom: 20upx;}
	.arrow{padding-right: 36upx;}
	.position_quan{position: relative;}
	.button_kefu {
	  background: transparent;
	  padding: 0;
	  margin: 0;
	  border-radius: 0;
	  width: 230upx;
	  height: 200upx;
	  position: absolute;
	  top: 0upx;
	  left: 0upx;
	}

	.button_kefu::after {
	  border: none;
	}
	
	.user-msg {
		display: flex;
		flex-direction: column;
		justify-content: space-between;
		text-align: left;
	}
	.user-msg>div{
		display: flex;align-items: center;
	}
	.user-msg>div .span{font-size: 30upx;color: #020202;font-weight: bold;line-height:24upx;}
	.user-msg-div{margin-top: 15upx;}
	
	.tag {
		font-size: 24upx;
		vertical-align: middle;
		position: relative;
		display: inline-flex;
		align-items: center;
		justify-content: center;
		box-sizing: border-box;
		padding: 0upx 16upx;
		height: 48upx;
		font-family: Helvetica Neue, Helvetica, sans-serif;
		white-space: nowrap;
	}
	
	.panpel {
		background-repeat: no-repeat;
		background-size: 100%;
	}
	
	.vip-flag {
		font-size: 20upx;
		width: 90upx;
		height: 30upx;
		border-radius: 6upx;
		margin-top: 10upx;
		color: #666;
	}
	
	.vip-flag::after {
		content: " ";
		width: 200%;
		height: 200%;
		position: absolute;
		top: 0;
		left: 0;
		border: 1upx solid currentColor;
		transform: scale(0.5);
		transform-origin: 0 0;
		box-sizing: border-box;
		border-radius: inherit;
		z-index: 1;
		pointer-events: none;
	}
	
	.vip-join {
		 background: linear-gradient(to right, #D1B686 , #F6E8CE);
		 border-radius: 5000upx;
		 color: #242424;
		 font-size: 22upx;
		 box-shadow: 0 4upx 4upx rgba(0, 0, 0, 0.2);
	}
	
	.level-tag {
		font-size: 20upx;
		font-weight: 500;
		height: 30upx;
		border-radius: 5upx;
		margin-left: 19upx;
		padding: 6upx 8upx;
	}
	
	.ren-ree {
		font-size: 22upx;
		font-weight: 500;
		line-height: 22upx;
	}
	
	.ren-ree-time {
		font-size:22upx;
		font-weight:500;
		line-height:22upx;
		margin-right:19upx;
	}
	
	.vip-text{
		height: auto;
		padding: 6upx 20upx;
		text-align: center;
		border-radius: 8upx;
		letter-spacing: 0.08rem;
	}
	.vip-text .img{margin:0;width: 21upx;height: 17upx;margin-right: 9upx;}
	
	.vip-text1{
		padding: 6upx 12rpx;
		text-align: center;
		background:rgba(238,238,238,1);border-radius:5rpx;border: 0;
		box-sizing: border-box;margin-left: 20rpx;
		font-size: 20rpx;
		color: #999999;
	}
	.vip-text1 .img1{margin:0;width: 18upx;height: 15upx;margin-right: 7upx;}
	.vip-core{
		padding: 5px 10px;
		height: auto;
		max-height: 52rpx;
	}
	
	.vip_modal{position: fixed;top: 0;left: 0;width: 100%;height: 100%;background: rgba(0, 0, 0, 0.4);z-index: 999999;}
	.vip_modal_data{position: absolute;top: 50%;margin-top: -369upx;left: 8upx;width: 663upx;height: 738upx; background-position: center; background-size: 100%;background-repeat: no-repeat;padding: 142upx 34upx 0  100upx;}
	.vip_modal_close{position: absolute;bottom: -110upx;left: 343upx;width: 50upx;height: 50upx;}
	.vip_expire{text-align: center;font-size: 36upx;background:linear-gradient(27deg,rgba(142,91,64,1) 0%, rgba(122,72,46,1) 100%); -webkit-background-clip:text; -webkit-text-fill-color:transparent;line-height:55upx;margin-bottom: 136upx;}
	.vip_expire p,.vip_act p{font-weight: bold;}
	.vip_act{display: flex;font-size: 24upx;color: #86593D;line-height:36upx;justify-content: space-between;padding: 0 56upx;}
	.vip_modal_data>p{text-align: center; font-size: 30upx; color: #93644A; font-weight: bold; margin-top: 154upx;}
	
