	.core_two img{border-radius: 50%;margin-bottom: 10upx;}
	.balance{width: 690upx;height: 140upx;background-color: #BE9E5F;margin: 0 auto;position:absolute;bottom: -60upx;left:4%;
			justify-content: space-between;padding: 0 20upx;font-size: 32upx;border-radius:20upx;}
	.balance_two{width: 140upx;height: 42upx;line-height:40upx;background-color: #242424;border-radius: 22upx;text-align: center;font-size: 24upx;color: #E5D1AD;}
	
	.order{padding: 0 30upx ;font-size: 26upx;color: #9D9D9D;background-color: #fff;margin-bottom: 20upx;}
	.order_one{justify-content: space-between;}
	.order_one img{margin-left: 12upx;}
	.order_one img{width: 16upx ;height: 28upx;vertical-align: sub;}
