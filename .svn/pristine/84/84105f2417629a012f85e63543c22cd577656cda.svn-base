.mask{height: 100vh;width: 100%;background-color: rgba(000,000,000,0.5);position: fixed;top: 0;left: 0;z-index: 34;
	display: flex;justify-content: center;align-items: center;}
.mask_cont{text-align: center;font-size: 30upx;color:#020202;background-color: #fff;border-radius: 10upx;}
.mask_cont>p{margin: 38upx 0;font-size: 30upx;}
.mask_cont>input{width: 500upx;padding-left: 20upx;height: 66upx;border-radius: 10upx;margin: 0 40upx 30upx 40upx;border:1px solid #b8b8b8;}
.mask_button{border-top: 1px solid #eee;padding: 10upx 0;height: 98upx;}
.mask_button>div{float: left;width: 50%;height: 78upx;color: #020202;display: flex;align-items: center;justify-content: center;font-size: 30upx;}
.mask_button:after{display: block;content: "";clear: both;}
.mask_button_left{border-right: 1px solid #eee;color: #9D9D9D!important;}
.jiantouDiv{position: absolute;right: 20upx;width: 40upx;height: 100%;display: flex;align-items: center;justify-content: center;z-index: 5;}
.jiantouDiv img{width: 16upx;height: 28upx;}
.warnImg{width: 24upx;height: 24upx;margin-right: 10upx;}
.formDiv{border-bottom: 1px solid #E9ECEF;}
.formList{display: flex;align-items: center;width: 100%;}
.leftText{height: 60upx;display: flex;align-items: center;width: 30%;font-size: 28upx;justify-content: flex-start;}
.rightInput{height: 60upx;display: flex;align-items: center;width: 70%;position: relative;padding-right: 30upx;}
.leftText1{height: 90upx;display: flex;align-items: center;width: 30%;font-size: 28upx;justify-content: flex-start;}
.rightInput1{height: 90upx;display: flex;align-items: center;width: 70%;position: relative;justify-content: flex-end;padding-right: 30upx;}
.rightInput1 span,.rightInput1 .text{margin-right: 40upx;font-size: 28upx;color: #999;word-wrap:break-word;width: 434upx;text-align: right;}
.rightInput1 div{font-size: 28upx;color: #020202;}
.rightInput input{font-size: 24upx;width: 100%;}
.rightInput1 input{font-size: 24upx;width: 100%;}
.warningText{display: flex;align-items: center;width: 100%;height: 30upx;font-size: 24upx;color: red;width: 70%;margin-left: 30%;margin-bottom: 20upx;}
.must{color: red;margin-right: 10upx;}

.container{height: 100vh;}
.head_img{border-radius: 50%;width: 90upx;height: 90upx;margin-right: 40upx;}
.pl_30{padding:0 0 0 30upx;}
.h_120{height: 120upx;}
.border_0{border: none;}
.hr{background-color: #f6f6f6;height: 20upx;}

.dianpuDiv{height: auto;min-height: 90upx;}